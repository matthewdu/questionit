<% provide(:title, "Userlist") %>
<h1>All Users</h1>

<%= will_paginate %>

<ul>
	<% @users.each do |user| %>
		<li>
			<%= user.id %>
			<%= link_to(user.username,user) %>
			<%= user.name %>
			<% if current_user.admin? && !current_user?(user) %>
		    	| <%= link_to "delete", user, method: :delete, data: { confirm: "You sure?" } %>
  			<% end %>
		</li>
	<% end %>
</ul>

<%= will_paginate %>